<xsl:stylesheet version= "1.0"
xmlns:xsl="https://ww.w3.org/1999/XSL/Transform">
<xsl:template match="/students">
<html><body>
<h2>Student List</h2>
<xsl:for-each select="student">
<p>
<xsl:value-of select="name"/>
<xsl:value-of select="course"/>
</p>
</xsl:for-each>
</body></html>
</xsl:template>
</xsl:stylesheet>

